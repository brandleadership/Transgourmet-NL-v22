<div data-bsi-element="header-transgourmet-prodega" style="border-width: 0px !important;max-width: 740px;margin:auto">
    <table
        border="0"
        cellpadding="0"
        cellspacing="0"
        align="center"
        width="740"
        bgcolor="#f4f4f4"
        class="mobile-full-width"
       >
        <tbody>
            <tr>
                <td>
                    <table bgcolor="#f4f4f4"
                        align="center"
                        border="0"
                        cellpadding="0"
                        cellspacing="0"
                        width="540" class="mobile-full-width" >
                        <tbody>
                            <tr>
                                <td colspan="3"
                                    height="20"
                                    width="100%"></td>
                            </tr>
                            <tr>
                                <td valign="top"
                                    width="540">
                                    <table align="center"
                                        border="0"
                                        cellpadding="0"
                                        cellspacing="0"
                                        width="100%"  class="mobile-full-width" >
                                        <tbody>
                                            <tr>
                                                <td width="100%">
                                                    <table align="left"
                                                        border="0"
                                                        cellpadding="0"
                                                        cellspacing="0"
                                                        width="100%"  class="mobile-full-width" >
                                                        <tbody>
                                                            <tr>
                                                                <td style="border-width: 0px !important;margin-top: 0px!important;margin-bottom: 0px !important;"
                                                                    valign="top"
                                                                    width="70%"
                                                                    align="left"
                                                                 
                                                                    colspan="2"
                                                                    valign="middle"
                                                                    height="45"
                                                                    data-bsi-dropzone="logo">
                                                                    {% block logo %}
                                                                        {% include '../../elements/nl-header-logo-transgourmet_prodega/template.twig' %}
                                                                    {% endblock %}
                                                                </td>
                                                                <td style="border-width: 0px !important;margin-top: 0px!important;margin-bottom: 0px !important;"
                                                                    valign="middle"
                                                                    width="30%"
                                                                    align="right"
                                                                    data-bsi-dropzone="preheader">
                                                                    {% block preheader %}
                                                                        {% include '../../elements/nl-preheader/template.twig' %}
                                                                    {% endblock %}
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="3"
                                    height="20"
                                    width="100%"></td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</div>
