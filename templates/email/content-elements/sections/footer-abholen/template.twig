                                    style="padding: 50px 0px;" class="mobile-full-width">
                                    <table align="center"
                                        border="0"
                                        cellpadding="0"
                                        cellspacing="0"
                                        width="100%" 
                                       class="mobile-full-width"
                                       >
                                        <tbody>
                                            <tr>
                                                <td width="100%"   
                                                    style="padding: 50px 0px;">
                                                    <div style="border-width: 0px !important;margin-top: 0px!important;margin-bottom: 0px !important;"
