                                                                    <div style="border-width: 0px !important;color: #808080;word-break: normal;"
                                                                        data-bsi-dropzone="footerlegal-abholen">
                                                                        {% block footerlegal %}
                                                                            {% include '../../elements/footer-text/template.twig' %}
                                                                        {% endblock %}
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td height="35"
                                                                    style="line-height: 40px; font-size:40px;mso-table-lspace:0;mso-table-rspace:0;margin:0;">
