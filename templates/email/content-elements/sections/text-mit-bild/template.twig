<div class="block-wrapper"
    data-bsi-element="nl-content-container"
    style="border-width: 0px !important;">
    <table border="0"
        cellpadding="0"
        cellspacing="0"
         align="center"
        width="740"
        bgcolor="#f4f4f4" class="mobile-full-width">
        <tbody>
            <tr>
                <td>
                    <table align="center"
                        bgcolor="#f4f4f4"
                        border="0"
                        cellpadding="0"
                        cellspacing="0"
                       class="mobile-full-width"
                        width="540"
                        style="border:0;">
                        <tbody>
                            <tr>
                                <td width="540">
                                    <table align="left"
                                        width="540"
                                        border="0"
                                        cellpadding="0"
                                        cellspacing="0"
                                        class="mobile-full-width">
                                        <tbody>
                                            <tr>
                                                <td style="padding-top: 40px;"
                                                    align="center"
                                                    class="padlr20">
                                                    <img data-bsi-element-part="image"
                                                        alt=""
                                                        border="0"
                                                        width="540"
                                                        src="{{
                                                        src
                                                            ?
                                                            : bsi_cx_asset(
                                                                '../../../../../resources/whitelabel/Image-4_3.png'
                                                            )
                                                        }}"
                                                        style="display: block; width: 540px;"
                                                        class="mobile-image-fullwidth" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td height="20"
                                                    style="line-height: 20px; font-size:20px;mso-table-lspace:0;mso-table-rspace:0;margin:0;">
                                                    &nbsp;
                                                </td>
                                            </tr>
                                            <tr>
                                                <td valign="top"
                                                    class="fallback-font"
                                                    align="left"
                                                    style="padding-bottom: 50px">
                                                    <table align="center"
                                                        bgcolor="#f4f4f4"
                                                        border="0"
                                                        cellpadding="0"
                                                        cellspacing="0"
                                                        class="mobile-full-width"
                                                        width="540"
                                                        style="border:0;">
                                                        <tr>
                                                            <td valign="top"
                                                                class="padlr20">
                                                                <div class="fallback-font"
                                                                    style="font-family: 'Transgourmet','Trebuchet MS','sans-serif'; font-weight: 700; font-style:normal; color: #000000;line-height:32px; font-size: 32px;text-align: left;margin-bottom: 10px;"
                                                                    data-bsi-element-part="plain-text">
                                                                    Partner vom
                                                                    Nez Rouge
                                                                </div>
                                                                <div class="fallback-font"
                                                                    style="font-family: 'Transgourmet','Trebuchet MS','sans-serif';font-weight:500; font-style:normal;color: #000000;font-size: 20px;line-height:32px;text-align: left;"
                                                                    data-bsi-element-part="formatted-text">
                                                                    Lorem Ipsum
                                                                    is simply
                                                                    dummy text
                                                                    of the
                                                                    printing and
                                                                    typesetting
                                                                    industry.
                                                                    Lorem Ipsum
                                                                    has been the
                                                                    industry's
                                                                    standard
                                                                    dummy text
                                                                    ever since
                                                                    the 1500s,
                                                                    when an
                                                                    unknown
                                                                    printer took
                                                                    a galley of
                                                                    type and
                                                                    scrambled it
                                                                    to make a
                                                                    type
                                                                    specimen
                                                                    book
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="padding-top:30px;border-width: 0px !important;margin-top: 0px!important;margin-bottom: 0px !important;"
                                                                valign="top"
                                                                class="padt20"
                                                                align="left"
                                                                data-bsi-dropzone="button">
                                                                {% block button %}
                                                                    {% include '../../elements/nl-button/template.twig' %}
                                                                {% endblock %}
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</div>
