<div  data-bsi-element="nl-spalten-combined" style="border-width: 0px !important;">
   <table border="0"
      cellpadding="0"
      bgcolor="#f4f4f4"
      cellspacing="0"
      class="mobile-full-width"
      align="center"
      width="740">
      <tbody>
         <tr>
            <td width="540" class="padding-mobile mobile-full-width"
               style="padding-bottom: 32px;">
               <table align="center"
                  border="0"
                  cellpadding="0"
                  cellspacing="0"
                  class="mobile-full-width"
                  width="540"
                  bgcolor="{{
                  styles['background-content']
                  }}">
                  <tbody>
                     <tr>
                        <td
                           class="mobile-full-width"
                           data-bsi-element-part="formatted-text">
                           <p
                              style="font-family: 'Transgourmet','Trebuchet MS', 'sans-serif';font-weight:bold; font-style:normal; color: #000000;line-height:28px; font-size: 26px;">
                              {{
                              properties.trans.product_list_title
                              }}
                           </p>
                        </td>
                     </tr>
                     <tr>
                        <td width='100%' height="30"></td>
                     </tr>
                     <tr>
                        <td>
                           <table align="center"
                              border="0"
                              cellpadding="0"
                              cellspacing="0"
                              width="540"
                              class="mobile-full-width"
                              bgcolor="{{
                              styles['background-content']
                              }}">
                              <tbody>
                                 <tr class="row-block">
                                   
                                             <td width="246"
                                                bgcolor="#ffffff"
                                                style="border: 1px solid #e5e5e5;width:246px;"
                                                class="mobile-full-width-table"
                                                valign="top"
                                                data-bsi-dropzone="nl-spalten-left">
                                                {% include '../../elements/single-product/template.twig' %}
                                             </td>
                                            
                                                <td width="48" height="1" class="mobile-space-block">
                                                   <img src="{{
                                                      src
                                                      ?
                                                      : bsi_cx_asset(
                                                      '../../../../../resources/whitelabel/x.png'
                                                      )
                                                      }}"
                                                      height="1" />
                                                </td>
                                               
                                             <td width="246"
                                                bgcolor="#ffffff"
                                                style="border: 1px solid #e5e5e5;width:246px;"
                                                valign="top"
                                                class="mobile-full-width-table"
                                                data-bsi-dropzone="nl-spalten-right">
                                                {% include '../../elements/single-product/template.twig' %}
                                             </td>
                                             
                                 </tr>
                              </tbody>
                           </table>
                        </td>
                     </tr>
                     <tr>
                        <td height="48" style="line-height: 1;"></td>
                     </tr>
                     <tr>
                        <td>
                           <table align="center"
                              border="0"
                              cellpadding="0"
                              cellspacing="0"
                              width="540"
                              class="mobile-full-width"
                              bgcolor="{{
                              styles['background-content']
                              }}">
                              <tbody>
                                 <tr class="row-block">
                                  
                                             <td width="246"
                                                bgcolor="#ffffff"
                                                style="border: 1px solid #e5e5e5;width:246px;"
                                                valign="top"
                                                class="mobile-full-width-table"
                                                data-bsi-dropzone="nl-spalten-left-2">
                                                {% include '../../elements/single-product/template.twig' %}
                                             </td>
                                             
                                                <td width="48" height="1" class="mobile-space-block">
                                                   <img src="{{
                                                      src
                                                      ?
                                                      : bsi_cx_asset(
                                                      '../../../../../resources/whitelabel/x.png'
                                                      )
                                                      }}"
                                                      height="1" />
                                                </td>
                                              
                                             <td width="246"
                                                bgcolor="#ffffff"
                                                style="border: 1px solid #e5e5e5;"
                                                valign="top"
                                                class="mobile-full-width-table"
                                                data-bsi-dropzone="nl-spalten-right-2">
                                                {% include '../../elements/single-product/template.twig' %}
                                             </td>
                                            
                                 </tr>
                              </tbody>
                           </table>
                        </td>

                     </tr>
                     <tr>


                        <td height="48" width="48" style="line-height: 1;"></td>
                     </tr>
                     <tr>
                        <td width="246"
                           class="mobile-full-width"
                           valign="top"
                           data-bsi-dropzone="nl-spalten-double-card-comb">
                        </td>
                     </tr>
                      <tr>


                        <td height="48" width="48" style="line-height: 1;"></td>
                     </tr>
                     <tr>
                        <td width="246"
                           class="mobile-full-width"
                           valign="top"
                           data-bsi-dropzone="nl-spalten-double-card-comb-2">
                        </td>
                     </tr>
                      <tr>


                        <td height="48" width="48" style="line-height: 1;"></td>
                     </tr>
                     <tr>
                        <td width="246"
                           class="mobile-full-width"
                           valign="top"
                           data-bsi-dropzone="nl-spalten-double-card-comb-3">
                        </td>
                     </tr>
                  </tbody>
               </table>
            </td>

         </tr>
      </tbody>
   </table>
</div>